{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"no-param-reassign": [
			"error",
			{ "props": true, "ignorePropertyModificationsForRegex": ["^el"] }
		],
		"no-underscore-dangle": ["error", { "allowAfterThis": true }]
	}
}
